<%-include("partials/header"); -%>

<h1><%= compose %></h1>
<form action="/compose" method="POST">
 <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" placeholder="Name it" name="title">
</div>
<div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="entry" rows="8"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Send it</button>
</form>

<%-include("partials/footer"); -%>